{
  "files.associations": {
    ".babelrc": "jsonc",
    ".eslintrc": "jsonc",
    ".prettierrc": "jsonc",
    ".eslintignore": "ignore"
  },

  "editor.rulers": [80, 100],
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#000"
  },
  "javascript.validate.enable": false,

  "search.exclude": {
    ".git": true,
    ".eslintcache": true,
    "src/dist": true,
    "src/main.prod.js": true,
    "src/main.prod.js.map": true,
    "bower_components": true,
    "dll": true,
    "release": true,
    "node_modules": true,
    "npm-debug.log.*": true,
    "test/**/__snapshots__": true,
    "yarn.lock": true,
    "*.{css,sass,scss}.d.ts": true
  },
  "cSpell.words": [
    "aapt",
    "Aoid",
    "apksigner",
    "apktool",
    "asar",
    "autoload",
    "autosave",
    "autoupdate",
    "baseui",
    "Boid",
    "browserslist",
    "bson",
    "bundletool",
    "codegen",
    "codemirror",
    "compat",
    "Contexify",
    "digestalg",
    "Dirtable",
    "downloadjs",
    "EDBNOTREADY",
    "EMISMATCHRESOURCETYPE",
    "endregion",
    "ERESOURCEFILENOTEXIST",
    "ERESOURCENOTFOUND",
    "eslintignore",
    "EWORKSPACENOTREADY",
    "Fastify",
    "fffb",
    "ffprobe",
    "flexsearch",
    "FXXX",
    "gitdir",
    "gpmc",
    "Hant",
    "hookz",
    "immer",
    "inlist",
    "Interactable",
    "isbinaryfile",
    "jarsigner",
    "jbcz",
    "jibencaozuo",
    "jotai",
    "jszip",
    "keypass",
    "localstorage",
    "lokijs",
    "Mergeable",
    "msgpack",
    "napi",
    "nkeyin",
    "nsis",
    "odwyer",
    "openapi",
    "pmmmwh",
    "polyv",
    "prettierrc",
    "promiseify",
    "recative",
    "Rerender",
    "resourcemanager",
    "runtypes",
    "scroller",
    "selecto",
    "selfsigned",
    "showwavespic",
    "sigalg",
    "simplesort",
    "splited",
    "storepass",
    "Styletron",
    "subrip",
    "teamsupercell",
    "tempfile",
    "tinydate",
    "undefinedin",
    "unmaximize",
    "unselectable",
    "Uploaders",
    "uson",
    "varname",
    "webm",
    "webp",
    "welldone",
    "WORKDIR",
    "xxhashjs",
    "XYWH",
    "Xywhf",
    "YYYYMMDD",
    "zipalign"
  ],
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnType": true,
  "telemetry.enableCrashReporter": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
